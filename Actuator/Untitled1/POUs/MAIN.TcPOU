AdvancedFeedbackUsed : BOOL;
TimeoutErrorElapsed : TIME := T#2S;
TimeAdvanceFeedback   : TIME := T#3S;


END_VAR
]]></Declaration>
    <Implementation>
      <ST><![CDATA[Actuator();
