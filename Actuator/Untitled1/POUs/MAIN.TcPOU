IF advance THEN 
	Actuator.M_Advance();
	ADVANCE := FALSE;
ELSIF Retract THEN 
	Actuator.M_Retract();
	RETRACT := FALSE;
ELSIF Reset THEN 
	Actuator.M_Reset();
	RESET:= FALSE;
END_IF]]></ST>
